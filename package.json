{
  "name": "angular_project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "concurrently -n FRONT,API -c green,blue \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "ng serve",
    "start:backend": "node server.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "clean": "node -e \"try{require('fs').rmSync('node_modules',{recursive:true,force:true});}catch(e){}; try{require('fs').rmSync('package-lock.json',{force:true});}catch(e){}\"",
    "reinstall": "npm run clean && npm install",
    "audit:fix": "npm audit fix",
    "audit": "npm audit",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "19.2.18",
    "@angular/common": "19.2.18",
    "@angular/compiler": "19.2.18",
    "@angular/core": "19.2.18",
    "@angular/forms": "19.2.18",
    "@angular/material": "19.2.18",
    "@angular/platform-browser": "19.2.18",
    "@angular/platform-browser-dynamic": "19.2.18",
    "@angular/router": "19.2.18",
    "@prisma/client": "^7.3.0",
    "axios": "^1.13.3",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.3",
    "rxjs": "~7.8.0",
    "stripe": "^20.2.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.18",
    "@angular/cli": "19.2.18",
    "@angular/compiler-cli": "19.2.18",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "21.0.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prisma": "^7.3.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "8.46.4"
  },
